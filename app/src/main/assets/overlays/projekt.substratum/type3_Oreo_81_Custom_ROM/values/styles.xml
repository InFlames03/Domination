<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.DialogAlert" parent="@*android:style/Theme.Material.Dialog.Alert" />
    <style name="RestoreDialog" parent="@*android:style/Theme.Material.Dialog.Alert" />
    <style name="Platform.V21.AppCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:textColorHintInverse">#ffbbbbbb</item>
        <item name="android:textColorHint">#ffbbbbbb</item>
        <item name="android:textColorLink">@*android:color/accent_material_dark</item>
        <item name="android:textColorLinkInverse">@*android:color/accent_material_dark</item>
    </style>
    <style name="Widget.Design.FloatingActionButton" parent="@*android:style/Widget">
        <item name="android:background">@*projekt.substratum:drawable/design_fab_background</item>
        <item name="*projekt.substratum:backgroundTint">@*android:color/button_material_dark</item>
        <item name="*projekt.substratum:borderWidth">@*projekt.substratum:dimen/design_fab_border_width</item>
        <item name="*projekt.substratum:elevation">@*projekt.substratum:dimen/design_fab_elevation</item>
        <item name="*projekt.substratum:fabSize">auto</item>
        <item name="*projekt.substratum:pressedTranslationZ">@*projekt.substratum:dimen/design_fab_translation_z_pressed</item>
        <item name="*projekt.substratum:rippleColor">@*android:color/ripple_material_dark</item>
    </style>


</resources>
