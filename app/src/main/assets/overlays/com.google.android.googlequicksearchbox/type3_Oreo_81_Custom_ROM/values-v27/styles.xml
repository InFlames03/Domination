<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Gsa" parent="@*com.google.android.googlequicksearchbox:style/Theme.Shim">
        <item name="android:textColorHighlight">@*com.google.android.googlequicksearchbox:color/qp_highlight</item>
        <item name="android:actionBarStyle">@*com.google.android.googlequicksearchbox:style/Gsa</item>
        <item name="android:alertDialogTheme">@*com.google.android.googlequicksearchbox:style/Theme.Gsa.Dialog</item>
        <item name="android:textColorHighlightInverse">@*com.google.android.googlequicksearchbox:color/qp_highlight</item>
        <item name="android:colorControlActivated">@*com.google.android.googlequicksearchbox:color/qp_blue</item>
        <item name="android:colorPrimary">@*com.google.android.googlequicksearchbox:color/qp_blue</item>
        <item name="android:colorPrimaryDark">@*android:color/black</item>
        <item name="android:colorAccent">@*com.google.android.googlequicksearchbox:color/qp_blue</item>
        <item name="android:navigationBarColor">@*com.google.android.googlequicksearchbox:color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:qpActionIconAlpha">1.0</item>
        <item name="*com.google.android.googlequicksearchbox:qpActionIconTint">@*android:color/white</item>
        <item name="*com.google.android.googlequicksearchbox:qpH1FontFamily">sans-serif-light</item>
        <item name="*com.google.android.googlequicksearchbox:qpH1TextSize">@*com.google.android.googlequicksearchbox:dimen/qp_h1_text_size</item>
        <item name="*com.google.android.googlequicksearchbox:vogPromptColor">@*com.google.android.googlequicksearchbox:color/qp_text_b2</item>
        <item name="*com.google.android.googlequicksearchbox:vogPromptTextStyle">2</item>
    </style>


    <style name="Theme.DynamicHost" parent="@*com.google.android.googlequicksearchbox:style/Theme.Shim">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:navigationBarColor">#ff000000</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>


    <style name="OpaHQActivity.Theme" parent="@*com.google.android.googlequicksearchbox:style/OpaHQActivity.Theme.Base">
        <item name="android:navigationBarColor">@*com.google.android.googlequicksearchbox:color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.AssistantSettings" parent="@*com.google.android.googlequicksearchbox:style/Theme.AssistantSettings.Base">
        <item name="android:navigationBarColor">@*com.google.android.googlequicksearchbox:color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>


    <style name="Theme.AssistantSettings.NoActionBar" parent="@*com.google.android.googlequicksearchbox:style/Theme.AssistantSettings">
        <item name="android:navigationBarColor">@*com.google.android.googlequicksearchbox:color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:windowActionBar">false</item>
        <item name="*com.google.android.googlequicksearchbox:windowNoTitle">true</item>
    </style>
    <style name="TransparentMonetActivity">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">adjustNothing|stateUnchanged</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/black</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
</resources>
